{
	"name": "abacus-ext-service-dashboard",
	"description": "CF Service Dashboard for Abacus",
	"license": "Apache-2.0",
	"version": "0.0.6-dev.9",
	"private": true,
	"homepage": "https://github.com/cloudfoundry-incubator/cf-abacus-broker/lib/cf/dashboard",
	"bugs": {
		"url": "https://github.com/cloudfoundry-incubator/cf-abacus-broker/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/cloudfoundry-incubator/cf-abacus-broker.git"
	},
	"keywords": [
		"cf",
		"abacus",
		"dashboard"
	],
	"files": [
		".apprc",
		"manifest.yml",
		"src/"
	],
	"main": "src/index.js",
	"scripts": {
		"start": "launch webapp start",
		"stop": "webapp stop",
		"lint": "eslint",
		"pub": "publish",
		"cfpack": "cfpack",
		"cfpush": "cfpush",
		"postinstall": "if ! bower install --allow-root 2>/dev/null; then a=$(pwd); cd ../../lib/cf/dashboard; bower install --allow-root; b=$(pwd); cd $a; cp -R $b/src/webapp/libs $a/src/webapp/libs; fi",
		"jsdoc": "jsdoc -c jsdoc.json",
		"jshint": "jshint --exclude coverage,docs,jsdoc,node_modules,webapp,test,tmp  --reporter=node_modules/jshint-stylish .",
		"jshint-ci": "jshint --exclude coverage,docs,jsdoc,node_modules,public,tmp --reporter=checkstyle .",
		"watch": "mocha src/test/backend --watch",
		"jsformat": "js-beautify -r *.js -r src/*.js -r src/lib/*.js -r src/lib/**/*.js -r src/test/*.js -r src/test/**/*.js",
		"test": "mocha src/test/*-test.js && karma start src/test/frontend/karma.conf.js && if [ -d ./.coverage ] ; then npm run combinecoverage; fi ",
		"combinecoverage": "cat src/test/frontend/coverage/lcov.info >> ./.coverage/lcov.info && node --harmony src/test/combineJson.js",
		"backend-coverage": "node ./node_modules/istanbul-harmony/lib/cli.js cover  --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --ui bdd -R spec -t 5000 src/test/backend/ && node --harmony ./node_modules/istanbul-harmony/lib/cli.js report --dir src/test/backend/coverage --root coverage html",
		"coverage": "karma start src/test/frontend/karma.conf.js && node ./node_modules/istanbul-harmony/lib/cli.js cover  --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --ui bdd -R spec -t 5000 src/test/backend/ && node --harmony ./node_modules/istanbul-harmony/lib/cli.js report --dir src/test/backend/coverage --root coverage html && node --harmony src/test/computeCoverage.js"
	},
	"dependencies": {
		"abacus-cluster": "file:../../../abacus/lib/utils/cluster",
		"abacus-debug": "file:../../../abacus/lib/utils/debug",
		"abacus-launcher": "file:../../../abacus/lib/utils/launcher",
		"abacus-webapp": "file:../../../abacus/lib/utils/webapp",
		"bluebird": "3.4.6",
		"cfenv": "1.0.3",
		"bower": "1.8.0",
		"express": "4.15.2",
		"express-session": "1.15.2",
		"request": "2.81.0",
		"passport-oauth2": "1.4.0",
		"passport": "0.3.2",
		"body-parser": "1.17.1",
		"winston": "2.3.1",
		"lodash": "4.13.1",
		"pug": "2.0.0-beta3",
		"https-proxy-agent": "1.0.0"
	},
	"devDependencies": {
		"abacus-cfpack": "file:../../../abacus/tools/cfpack",
		"abacus-cfpush": "file:../../../abacus/tools/cfpush",
		"abacus-eslint": "file:../../../abacus/tools/eslint",
		"abacus-mocha": "file:../../../abacus/tools/mocha",
		"abacus-publish": "file:../../../abacus/tools/publish",
		"istanbul-harmony": "0.3.16",
		"chai": "3.5.0",
		"chai-http": "3.0.0",
		"js-beautify": "1.6.4",
		"jsdoc": "3.4.0",
		"jshint": "2.9.3",
		"jshint-stylish": "2.2.0",
		"mocha": "2.5.3",
		"nock": "9.0.2",
		"proxyquire": "1.5.0",
		"sinon": "1.17.5",
		"sinon-chai": "2.8.0",
		"grunt": "1.0.1",
		"karma": "1.5.0",
		"karma-phantomjs-launcher": "1.0.3",
		"karma-read-json": "1.1.0",
		"karma-jasmine": "1.1.0",
		"karma-cli": "1.0.1",
		"karma-coverage": "1.1.1",
		"grunt-contrib-concat": "1.0.1",
		"grunt-contrib-cssmin": "1.0.2",
		"grunt-contrib-jshint": "1.0.0",
		"grunt-contrib-less": "1.4.0",
		"grunt-contrib-uglify": "2.0.0",
		"grunt-contrib-watch": "1.0.0",
		"grunt-karma": "2.0.0",
		"grunt-ng-annotate": "3.0.0",
		"grunt-npm-install": "0.3.1",
		"grunt-run": "0.6.0",
		"jsdom": "9.12.0",
		"jquery": "3.2.1"
	},
	"engines": {
		"node": ">=6.10.0",
		"npm": "<5.0.0"
	}
}